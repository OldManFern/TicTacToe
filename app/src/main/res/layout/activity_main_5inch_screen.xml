<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    >


    <TextView
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/In_app_title"
        android:id="@+id/textView"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_columnSpan="5"
        android:gravity="center" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnTopLeft"
        android:background="@android:color/transparent"
        android:layout_row="1"
        android:layout_column="0"

        />

    <ImageView
        android:layout_width="5dp"
        android:layout_height="227dp"
        android:id="@+id/imageView"
        android:layout_row="1"
        android:layout_column="1"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:layout_rowSpan="5"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnTopCenter"
        android:background="@android:color/transparent"
        android:layout_row="1"
        android:layout_column="2" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="5dp"
        android:id="@+id/imageView2"
        android:layout_row="2"
        android:layout_column="0"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="5dp"
        android:id="@+id/imageView3"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="5dp"
        android:id="@+id/imageView5"
        android:layout_row="2"
        android:layout_column="4"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnMidLeft"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="3"
        android:layout_column="0" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnMidCenter"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="3"
        android:layout_column="2" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnMidRight"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="3"
        android:layout_column="4" />

    <ImageView
        android:layout_width="5dp"
        android:layout_height="227dp"
        android:id="@+id/imageView4"
        android:layout_row="1"
        android:layout_column="3"
        android:minHeight="40dp"
        android:minWidth="40dp"
        android:background="#ff000000"
        android:layout_rowSpan="5"
        android:contentDescription="@string/spacing" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnTopRight"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="1"
        android:layout_column="4"
        android:layout_rowWeight="1" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="5dp"
        android:id="@+id/imageView6"
        android:layout_row="4"
        android:layout_column="0"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="5dp"
        android:id="@+id/imageView7"
        android:layout_row="4"
        android:layout_column="2"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <ImageView
        android:layout_width="104dp"
        android:layout_height="5dp"
        android:id="@+id/imageView8"
        android:layout_row="4"
        android:layout_column="4"
        android:background="#ff000000"
        android:contentDescription="@string/spacing" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnBotLeft"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="5"
        android:layout_column="0" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnBotCenter"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="5"
        android:layout_column="2" />

    <Button
        android:layout_width="104dp"
        android:layout_height="72dp"
        android:id="@+id/btnBotRight"
        android:background="@android:color/transparent"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_row="5"
        android:layout_column="4" />

    <TextView
        android:layout_width="326dp"
        android:layout_height="183dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/initial_output_message"
        android:id="@+id/outputMessages"
        android:layout_row="6"
        android:layout_column="0"
        android:layout_columnSpan="5"
        android:gravity="center"
        android:textSize="40sp" />

</GridLayout>
